<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.filmfilter"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/film_filter_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/film_filter_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.FilmFilter"
        tools:targetApi="31"
        android:name=".MyApplication">
        <activity
            android:name=".View.InicioSesionActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.PasswordActivity"/>
        <activity android:name=".View.SearchActivity"/>
        <activity android:name=".View.RegistroActivity"/>
        <activity android:name=".View.PreferenciasActivity"/>
        <activity android:name=".View.DetallesPeliActivity"/>
        <activity android:name=".View.HomeActivity"/>
        <activity android:name="com.example.filmfilter.View.FavoritosActivity"/>
        <activity android:name=".View.EditarPreferenciasActivity"/>
    </application>


</manifest>